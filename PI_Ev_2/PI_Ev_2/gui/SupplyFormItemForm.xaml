<Window x:Class="PI_Ev_2.gui.SupplyFormItemForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Formulario de Avituallamiento" Height="114" Width="272"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize">
    <Grid>
        <ComboBox x:Name="cbItems" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="226"/>
        <Button x:Name="btnCancel" Content="Cancelar" HorizontalAlignment="Left" Margin="161,46,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        <Button x:Name="btnAdd" Content="Añadir" HorizontalAlignment="Left" Margin="81,46,0,0" VerticalAlignment="Top" Width="75" Click="btnAdd_Click"/>

    </Grid>
</Window>
