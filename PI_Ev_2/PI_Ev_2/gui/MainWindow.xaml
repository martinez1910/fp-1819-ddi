<Window x:Class="PI_Ev_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Avituallamiento 2.0" Height="140" Width="296"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanMinimize">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="483">
            <MenuItem Header="MenuItem">
                <MenuItem Header="MenuItem" Height="100" Width="100"/>
            </MenuItem>
        </Menu>
        <Button x:Name="BtnRaceManager" Content="Carreras..." HorizontalAlignment="Left" Margin="10,25,0,0" VerticalAlignment="Top" Width="75" Click="BtnRaceManager_Click"/>
        <Button x:Name="BtnSupplyManager" Content="Avituallamiento..." HorizontalAlignment="Left" Margin="90,25,0,0" VerticalAlignment="Top" Width="100" Click="BtnSupplyManager_Click"/>
        <Button x:Name="BtnItemsManager" Content="Material..." HorizontalAlignment="Left" Margin="195,25,0,0" VerticalAlignment="Top" Width="75" Click="BtnItemsManager_Click"/>
        <Button x:Name="BtnExit" Content="Salir" HorizontalAlignment="Left" Margin="195,70,0,0" VerticalAlignment="Top" Width="75" Click="BtnExit_Click"/>

    </Grid>
</Window>
